   "metadata": {},
   "outputs": [],
   "source": [
    "x=np.linspace(-20,20,100)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "34b426c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "y=[]\n",
